algoritmo "semnome"
// Função :
// Autor :
// Data : 19/10/2022
// Seção de Declarações 
var
    N, A, S: Inteiro
inicio
    Escreva ("Digite um número: ")
    Leia (N)
    
    A <- N - 1
    S <- N + 1
    Escreval ("O antecessor de ", N, " é ", A)
    EscrevaL ("O sucessor de ", N, " é ", S)

fimalgoritmo
