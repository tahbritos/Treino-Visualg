algoritmo "NomeSalario"

var
      Nome: caractere
      Salario: inteiro
inicio
      Escreva ("Nome do funcionário: ")
      Leia (Nome)
      Escreva ("Salário: ")
      Leia (salario)
      EscrevaL ("        ")
      EscrevaL ("O funcionário ", nome, " tem um salário de ", salario)
fimalgoritmo
