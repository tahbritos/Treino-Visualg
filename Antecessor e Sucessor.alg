algoritmo "AntecessorSucessor"


var
     N, A, S: Inteiro
inicio
     EscrevaL (". . . . . . . . . . . . . .")
     Escreva ("Digite um numero: ")
     Leia (N)

     EscrevaL (". . . . . . . . . . . . . .")
     A <- N - 1
     EscrevaL ("O antecessor de ", N, " é ", A)
     S <- N + 1
     EscrevaL ("O sucessor de ", N, " é ", S)
     
     
fimalgoritmo
